[USVISA]
; Account and current appointment info from https://ais.usvisa-info.com
USERNAME = example@example.com
PASSWORD = xxxxxxxxxx
; SCHEDULE_ID is in URL https://ais.usvisa-info.com/es-co/niv/schedule/SCHEDULE_ID/continue_actions
; SCHEDULE_ID is not IVR Account Number
SCHEDULE_ID = 99999999
; In case you required an appointment on ASC
NEED_ASC = False
; English - Canada
COUNTRY_CODE = en-ca
; Vancouver
FACILITY_ID = 95
; NOT BEING USED - Vancouver
ASC_ID = 95

[CHROMEDRIVER]
; Way to use selenium LOCAL, AWS or REMOTE
USE = AWS
; Optional: HUB_ADDRESS is mandatory only when USE = REMOTE
HUB_ADDRESS = http://localhost:9515/wd/hub

[PUSHOVER]
; Get push notifications via https://pushover.net/ (optional)
PUSH_TOKEN = xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
PUSH_USER = xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

[SENDGRID]
; Get email notifications via https://sendgrid.com/ (optional)
